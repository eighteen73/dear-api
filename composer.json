{
    "name": "eighteen73/dear-api",
    "description": "PHP Library for dear systems API v2 https://dearinventory.docs.apiary.io",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Umair Mahmood",
            "email": "umair.mahmood22@gmail.com"
        },
        {
            "name": "Matt Jones",
            "email": "matt@eighteen73.co.uk"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.4",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Eighteen73\\Dear\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eighteen73\\Dear\\Test\\" : "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
